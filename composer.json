{
    "name": "mustorze/mustafilter",
    "description": "A Filter for Laravel",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "illuminate/support": "5.*|6.*|7.*|8.*",
        "webonyx/graphql-php": "^0.13",
        "laravel/framework": "5.*|6.*|7.*|8.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mustorze",
            "email": "mustorze@mustorze.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Mustorze\\MustAFilter\\": "src/"
        }
    }
}
